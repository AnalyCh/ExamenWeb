<div id="map_canvas"></div>
<iframe 
  width="300" 
  height="170" 
  frameborder="0" 
  scrolling="no" 
  marginheight="0" 
  marginwidth="0" 
  src="https://maps.google.com/maps?q=39.46910009461806,-0.39186259999996764&hl=es;z=14&amp;output=embed"
 >
 </iframe>
 <br />
 <small>
   <a 
    href="https://maps.google.com/maps?q=39.46910009461806,-0.39186259999996764&hl=es;z=14&amp;output=embed" 
    style="color:#0000FF;text-align:left" 
    target="_blank"
   >
     See map bigger
   </a>
 </small>

<%- include('librerias.ejs')%>

<script  src="http://maps.google.com/maps/api/js?sensor=false">

var myCenter=new google.maps.LatLng(39.46910009461806, -0.39186259999996764);
 
var marker = new google.maps.Marker({
        position: myCenter
        ,});
 
window.onload = function() {
        var mapOptions = {
          center: myCenter,
          zoom: 16,
          mapTypeId: google.maps.MapTypeId.ROADMAP,
          panControl: false,
          zoomControl: false,
          scaleControl: false,
          };
        var map = new google.maps.Map(document.getElementById("map_canvas"), mapOptions);
    marker.setMap(map);
        };

</script>
 


 



